<% if logged_in? %>
	<!-- Show logged in page.  -->
	<h1>TODO</h1>
	<h2>Show Clocking in View</h2>
	<h2>Show Announcements View</h2>
	<div id="widget" class="span5">
		<h2>Upcoming Shifts:</h2>
		<%for shift in current_user.get_next_week_user_shifts %>
			<ul><%=shift.starttime%></ul>
		<%end%>
	</div>
<% else %>
	<%= render :template => 'sessions/new' %>
<% end %>